{
  "name": "SCNRecorder",
  "version": "2.0.0",
  "summary": "A lags-free recorder of ARKit and SceneKit for iOS in Swift",
  "homepage": "https://github.com/gorastudio/SCNRecorder",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Vladislav Grigoryev": "dev.grigoriev@gmail.com"
  },
  "source": {
    "git": "https://github.com/gorastudio/SCNRecorder.git",
    "tag": "2.0.0"
  },
  "module_name": "SCNRecorder",
  "module_map": "SCNRecorder.modulemap",
  "swift_versions": "5.2",
  "platforms": {
    "ios": "11.0"
  },
  "default_subspecs": [
    "Core",
    "Scene"
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "SCNRecorder.h",
        "Sources/**/*.{h,m,swift}"
      ],
      "exclude_files": [
        "Sources/**/*Clean*.{h,m,swift}",
        "Sources/**/*Scene*.{h,m,swift}"
      ],
      "public_header_files": [
        "SCNRecorder.h",
        "Sources/**/*.h"
      ],
      "private_header_files": "Sources/**/*.h"
    },
    {
      "name": "Scene",
      "source_files": "Sources/**/*Scene*.{h,m,swift}"
    },
    {
      "name": "Clean",
      "source_files": "Sources/**/*Clean*.{h,m,swift}"
    }
  ],
  "swift_version": "5.2"
}
